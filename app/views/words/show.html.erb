<%= link_to "Back to #{@word.group.name}", @word.group %>
<h1><%= @word.name %></h1>
<h3>Definition</h3>
<p><%= @word.definition %></p>
<h3>Example</h3>
  <div class='word-meme-show'>
    <% @memes.each do |meme| %>
      <% if meme.binary %>
        <%= image_tag url_for(:controller => "memes", :action => "show_meme", :id => meme.id),
                              size: "525x450", alt:"Meme" %>
      <% end %>
    <% end %>
  </div>
