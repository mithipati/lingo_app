<%= form_for([@group, Word.new], remote: true) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :definition %>
    <%= f.text_area :definition %>

    <%= fields_for(@meme) do |fa| %>
      <%= fa.label :image %>
      <%= fa.collection_select(:image,@response.body,:to_s,:to_s) %>
      <%= fa.label :top %>
      <%= fa.text_field :top %>
      <%= fa.label :bottom %>
      <%= fa.text_field :bottom %>
    <% end %>
  <%= f.submit 'Create', class: "btn btn-large btn-primary"%>
<% end %>
