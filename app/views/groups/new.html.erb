<h1>New group</h1>
<div class="row">
  <div class="span6 offset3">
    <%= render 'shared/error_messages' %>
    <%= form_for(@group) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :group_pic, "Group Picture" %>
      <%= f.file_field :group_pic %>

      <%= f.label :description %>
      <%= f.text_field :description %>

      <%= f.submit 'Create', class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
